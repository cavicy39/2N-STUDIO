{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/valora-support", "destination": "/valora-support.html" },
    { "source": "/valora", "destination": "/valora.html" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
